{
    "extends": "../tsconfig.json",
  "compilerOptions": {
    "target": "ESNext",
    "module": "commonjs",
    "outDir": "../dist/main/",
    "rootDir": "./",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "moduleResolution": "node",
    "sourceMap": true,
    "baseUrl": ".",
    "paths": {
      "@library": [
        "./library.ts"
      ]
    }

  },
  "include": ["./**/*.ts", "./library.json"],
  "exclude": ["node_modules"]
}